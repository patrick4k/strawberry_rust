    "pattern": "{"
  },
  {
    "name": "CurlyBracketClose",
    "method": "Match",
    "pattern": "}"
  },
  {
    "name": "SquareBracketOpen",
    "method": "Match",
    "pattern": "["
  },
  {
    "name": "SquareBracketClose",
    "method": "Match",
    "pattern": "]"
  },
  {
    "name": "Period",
    "method": "Match",
    "pattern": "."
  },
  {
    "name": "Colon",
